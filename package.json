{
  "name": "anisuggest",
  "type": "module",
  "version": "1.0.0",
  "description": "A bot that goes through your anilist and recommends you anime and manga.",
  "author": "Yuuko Devs",
  "license": "MIT",
  "keywords": [
    "anilist",
    "manga",
    "anime",
    "suggest"
  ],
  "main": "src/app.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.js --env production",
    "dev": "bun run --watch ./src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix", 
    "gen:gql": "graphql-codegen",
    "gen:queries": "bun run ./Scripts/genQueries.ts",
    "db:push": "bunx drizzle-kit push:sqlite"
  },
  "dependencies": {
    "@acegoal07/discordjs-pagination": "^1.5.0",
    "@discordjs/rest": "^0.3.0",
    "@types/dotenv-flow": "^3.3.1",
    "@types/node-rsa": "^1.1.2",
    "axios": "^0.21.1",
    "better-sqlite3": "^9.0.0",
    "bun-types": "^1.0.5",
    "canvas": "^2.11.2",
    "discord-api-types": "^0.27.3",
    "discord.js": "^14.7.1",
    "dotenv-flow": "^3.2.0",
    "drizzle-orm": "^0.28.6",
    "humanize-duration": "^3.27.0",
    "ms": "^2.1.3",
    "node-rsa": "^1.1.1",
    "nodemon": "^2.0.12",
    "sequelize": "^6.17.0",
    "turndown": "^7.1.1",
    "yarn": "^1.22.11"
  },
  "imports": {
    "#Utils/*": [
      "./src/Utils/*"
    ],
    "#Commands/*": [
      "./src/Commands/*"
    ],
    "#Structures/*": [
      "./src/Structures/*"
    ],
    "#GraphQL/*": [
      "./src/GraphQL/*"
    ],
    "#Models/*": [
      "./src/Database/Models/*"
    ],
    "#Caching/*": [
      "./src/Caching/*"
    ],
    "#RSA/*": [
      "./src/RSA/*"
    ],
    "#Middleware/*": [
      "./src/Middleware/*"
    ],
    "#Components/*": [
      "./src/Components/*"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.26",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/humanize-duration": "^3.27.1",
    "@types/node": "^20.6.3",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "typescript": "^5.2.2"
  }
}
